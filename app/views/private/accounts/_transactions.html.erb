<table class="table">
  <thead>
    <tr>
      <th>Account</th>
      <th>Amount</th>
      <th>Code</th>
      <th>Comment</th>
      <th>Created</th>
    </tr>
  </thead>
  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.account %></td>
        <td><%= transaction.amount %></td>
        <td><%= transaction.code %></td>
        <td><%= transaction.comment %></td>
        <td><%= l(transaction.created_at, :format => :short) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
